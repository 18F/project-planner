{
  "name": "18f/project-planner",
  "type": "project",
  "description": "Simple project planner for inventorying current and future projects and staying current about what is going on in the organization related to project planning.",
  "keywords": ["project", "planning", "collaboration", "coordination", "notifications", "inventory", "discovery"],
  "license": "CC0-1.0",
  "authors": [
    {
      "name": "Adrian Webb",
      "email": "adrian.webb@gsa.gov",
      "role": "Whatever is needed"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url":  "https://packages.drupal.org/8"
    },
    {
      "type": "vcs",
      "url": "https://github.com/backupmigrate/backup_migrate_core"
    }
  ],
  "require": {
    "composer/installers": "*",
    "oomphinc/composer-installers-extender": "*",
    "cweagans/composer-patches": "~1.0",
    "backupmigrate/core": "dev-master",
    "psr/log": "dev-master",
    "drupal/console": "~0.10",
    "drush/drush": "~8.1",
    "drush/config-extra": "1.0.*",
    "drupal/core": "8.1.*",
    "drupal/deploy": "8.1.*",
    "drupal/relaxed": "8.1.*",
    "drupal/multiversion": "8.1.x-dev",
    "drupal/backup_migrate": "8.4.*",
    "drupal/field_group": "8.1.*",
    "drupal/entity_reference_revisions": "8.1.*",
    "drupal/page_manager": "8.1.*",
    "drupal/panelizer": "8.3.*",
    "drupal/flysystem": "8.1.*",
    "drupal/flysystem_s3": "8.1.*",
    "drupal/rules": "8.3.*",
    "drupal/search_api": "8.1.*",
    "drupal/facets": "8.1.*",
    "drupal/menu_link_config": "8.*",
    "drupal/bootstrap": "3.0.x-dev",
    "drupal/devel": "1.0.x-dev",
    "drupal/bootstrap_layouts": "^3.1",
    "drupal/default_content": "1.0.x-dev",
    "drupal/csv_serialization": "1.0.x-dev",
    "drupal/restui": "^1.11",
    "league/csv": "^7.1",
    "drupal/simple_oauth": "1.0.x-dev"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "symfony/css-selector": "2.7.*"
  },
  "conflict": {
     "drupal/drupal": "*"
  },
  "autoload": {
    "psr-4": {
      "BackupMigrate\\Drupal\\": "src"
    },
    "classmap": [
      "scripts/composer/DrupalHandler.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "DrupalProject\\composer\\DrupalHandler::initializeFiles",
      "DrupalProject\\composer\\DrupalHandler::setHashSalt",
      "DrupalProject\\composer\\DrupalHandler::buildTheme",
      "DrupalProject\\composer\\DrupalHandler::initializeSite",
      "DrupalProject\\composer\\DrupalHandler::syncConfigurations"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\DrupalHandler::initializeFiles",
      "DrupalProject\\composer\\DrupalHandler::setHashSalt",
      "DrupalProject\\composer\\DrupalHandler::buildTheme",
      "DrupalProject\\composer\\DrupalHandler::initializeSite",
      "DrupalProject\\composer\\DrupalHandler::syncConfigurations"
    ]
  },
  "extra": {
    "installer-types": [],
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/libraries/{$name}/": ["type:drupal-library"],
      "web/modules/contrib/{$name}": ["type:drupal-module"],
      "web/profiles/contrib/{$name}": ["type:drupal-profile"],
      "web/themes/contrib/{$name}": ["type:drupal-theme"],
      "drush/contrib/{$name}": ["type:drupal-drush"]
    }
  }
}
